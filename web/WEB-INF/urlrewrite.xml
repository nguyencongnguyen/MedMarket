<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
    <rule>
        <from>^(/|/home)(/|)$</from>
        <to>/home.action</to>
    </rule>
    <rule>
        <from>^(/|/home)$</from>
        <to>/main/home.action</to>
    </rule>
    <rule>
        <from>^(/video)$</from>
        <to>/video/view.action</to>
    </rule>
    <rule>
        <from>^/video/(\w*(\-?\w*)*)$</from>
        <to>/video/play.action?id=$1</to>
    </rule>
    <rule>
        <from>^/video/(\w*(\-?\w*)*)\?(.*)$</from>
        <to>/video/$1.action?$2</to>
    </rule>
    <rule>
        <from>^/free/(\w*(\-?\w*)*)$</from>
        <to>/free/$1.action</to>
    </rule>
    <rule>
        <from>^/home/(\w*(\-?\w*)*)$</from>
        <to>/main/$1.action</to>
    </rule>
    <rule>
        <from>^/include/(\w*(\-?\w*)*)$</from>
        <to>/free/bookingInclude.action?username=$1</to>
    </rule>
    <rule>
        <from>^/includeSubmit/bookingIncludeSubmit$</from>
        <to>/free/bookingIncludeSubmit.action</to>
    </rule>
    <rule>
        <from>^/home/detail/(\d+)$</from>
        <to>/main/booking.action?locationId=$1</to>
    </rule>
    <rule>
        <from>^/home/(\w*(\-?\w*)*)\?(.*)$</from>
        <to>/main/$1.action?$2</to>
    </rule>
    <rule>
        <from>^/(\w*(\-?\w*)*)$</from>
        <to>/main/booking.action?username=$1</to>
    </rule>
    <rule>
        <from>^(/admin)$</from>
        <to>/admin/home.action</to>
    </rule>
</urlrewrite>